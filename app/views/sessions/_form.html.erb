<%= form_with url: login_path do |f| %>
  <div class="form-group">
    <%= f.label :email, "Email" %>
    <%= f.text_field :email, placeholder: "Email" %>
  </div>
  <div class="form-group">
    <%= f.label :password, "Password" %>
    <%= f.text_field :password, placeholder: "Password" %>
  </div>
  <div class="form-group">
    <%= f.submit "Login" %>
    or
    <%= link_to  "Register", new_user_path %>
  </div>
<% end %>